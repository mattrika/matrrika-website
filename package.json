{
  "name": "mattrika-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "i": "npm i --legacy-peer-deps",
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve",
    "build": "ng build",
    "build:alternative": "NODE_ENV=production node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e:open": "npx cypress open",
    "e2e:run": "npx cypress run",
    "lint": "npx biome lint ./src --apply",
    "prepare": "husky",
    "format:ts": "npx biome format ./src --write",
    "format:html": "prettier --write 'src/**/*.{html,xml,scss,css,less}'",
    "format": "npm run format:ts && npm run format:html",
    "ng-update": "ng update @angular/core @angular/cli @angular/cdk @angular/material @schematics/angular --allow-dirty",
    "make-env-file": "tsc src/environments/make-env-file.ts && node src/environments/make-env-file.js && rm src/environments/make-env-file.js"
  },
  "dependencies": {
    "@angular/animations": "^18.2.4",
    "@angular/cdk": "^18.2.4",
    "@angular/common": "^18.2.4",
    "@angular/compiler": "^18.2.4",
    "@angular/core": "^18.2.4",
    "@angular/forms": "^18.2.4",
    "@angular/material": "^18.2.4",
    "@angular/platform-browser": "^18.2.4",
    "@angular/platform-browser-dynamic": "^18.2.4",
    "@angular/router": "^18.2.4",
    "@auth0/angular-jwt": "^5.2.0",
    "@ng-icons/core": "^29.5.0",
    "@ng-icons/heroicons": "^29.5.0",
    "@ng-web-apis/common": "^4.5.0",
    "flowbite": "^2.5.1",
    "prettier-eslint": "^16.3.0",
    "radash": "^12.1.0",
    "rxjs": "~7.8.1",
    "tailwind-merge": "^2.5.2",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.4",
    "@angular-eslint/builder": "18.3.1",
    "@angular-eslint/eslint-plugin": "18.3.1",
    "@angular-eslint/eslint-plugin-template": "18.3.1",
    "@angular-eslint/schematics": "18.3.1",
    "@angular-eslint/template-parser": "18.3.1",
    "@angular/cli": "^18.2.4",
    "@angular/compiler-cli": "^18.2.4",
    "@biomejs/biome": "1.9.1",
    "@schematics/angular": "^18.2.4",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.16.5",
    "@typescript-eslint/eslint-plugin": "8.5.0",
    "@typescript-eslint/parser": "8.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jasmine-core": "~5.3.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.11",
    "tailwindcss-elevation": "^2.0.0",
    "typescript": "~5.5.4"
  },
  "optionalDependencies": {
    "@biomejs/cli-darwin-x64": "1.9.1",
    "@biomejs/cli-linux-x64": "1.9.1",
    "@biomejs/cli-win32-x64": "1.9.1"
  },
  "lint-staged": {
    "src/**/*.{html,xml,scss,css,less}": "npx prettier --write",
    "src/**/*.{ts,js}": [
      "npx biome format --write",
      "npx biome lint --apply"
    ]
  }
}
