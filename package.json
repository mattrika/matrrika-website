{
  "name": "mattrika-website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "i": "npm i --legacy-peer-deps",
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve",
    "build": "ng build",
    "build:alternative": "NODE_ENV=production node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e:open": "npx cypress open",
    "e2e:run": "npx cypress run",
    "lint": "npx biome lint ./src --apply",
    "prepare": "husky",
    "format:ts": "npx biome format ./src --write",
    "format:html": "prettier --write 'src/**/*.{html,xml,scss,css,less}'",
    "format": "npm run format:ts && npm run format:html",
    "ng-update": "ng update @angular/core @angular/cli @angular/cdk @angular/material @schematics/angular --allow-dirty",
    "make-env-file": "tsc src/environments/make-env-file.ts && node src/environments/make-env-file.js && rm src/environments/make-env-file.js"
  },
  "dependencies": {
    "@angular/animations": "^19.0.4",
    "@angular/cdk": "^19.0.3",
    "@angular/common": "^19.0.4",
    "@angular/compiler": "^19.0.4",
    "@angular/core": "^19.0.4",
    "@angular/forms": "^19.0.4",
    "@angular/material": "^19.0.3",
    "@angular/platform-browser": "^19.0.4",
    "@angular/platform-browser-dynamic": "^19.0.4",
    "@angular/router": "^19.0.4",
    "@auth0/angular-jwt": "^5.2.0",
    "@ng-icons/core": "^29.10.0",
    "@ng-icons/heroicons": "^29.10.0",
    "@ng-web-apis/common": "^4.11.1",
    "@omnedia/ngx-background-beams": "^2.0.0",
    "@omnedia/ngx-border-beam": "^2.0.0",
    "@omnedia/ngx-gradient-text": "^2.0.0",
    "@omnedia/ngx-number-ticker": "^2.0.0",
    "@omnedia/ngx-shine-border": "^2.0.0",
    "@omnedia/ngx-three-globe": "^2.0.0",
    "flowbite": "^2.5.2",
    "prettier-eslint": "^16.3.0",
    "radash": "^12.1.0",
    "rxjs": "~7.8.1",
    "tailwind-merge": "^2.5.5",
    "three": "^0.167.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.4",
    "@angular-eslint/builder": "19.0.2",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/schematics": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/cli": "^19.0.4",
    "@angular/compiler-cli": "^19.0.4",
    "@biomejs/biome": "1.9.4",
    "@schematics/angular": "^19.0.4",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/jasmine": "~5.1.5",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "8.18.0",
    "@typescript-eslint/parser": "8.18.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.7",
    "jasmine-core": "~5.5.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.11",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.16",
    "tailwindcss-elevation": "^2.0.0",
    "typescript": "~5.6.3"
  },
  "optionalDependencies": {
    "@biomejs/cli-darwin-x64": "1.9.4",
    "@biomejs/cli-linux-x64": "1.9.4",
    "@biomejs/cli-win32-x64": "1.9.4"
  },
  "lint-staged": {
    "src/**/*.{html,xml,scss,css,less}": "npx prettier --write",
    "src/**/*.{ts,js}": [
      "npx biome format --write",
      "npx biome lint --apply"
    ]
  }
}
